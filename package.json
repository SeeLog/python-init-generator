{
	"name": "python-init-generator",
	"displayName": "Python init Generator",
	"description": "Generate Python __init__.py",
	"version": "0.0.14",
	"engines": {
		"vscode": "^1.74.0"
	},
	"publisher": "SeeLog",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/SeeLog/python-init-generator"
	},
	"license": "MIT",
	"activationEvents": [
		"onCommand:python-init-generator.generateInit"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "python-init-generator.generateInit",
				"title": "Python init Generator: Generate __init__.py"
			},
			{
				"command": "python-init-generator.generateInitFromLocation",
				"title": "Generate __init__.py from here"
			}
		],
		"configuration":[
			{
				"title": "Python Init Generator",
				"properties": {
					"pythonInitGenerator.ignoreDirectories": {
						"default": [],
						"type": "array",
						"description": "A list of directories names to ignore"
					}
				}
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "python-init-generator.generateInitFromLocation",
					"when": "false"
				}
			],
			"explorer/context": [
				{
					"command": "python-init-generator.generateInitFromLocation"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^8.2.3",
		"@types/node": "^10.17.60",
		"@types/vscode": "^1.74.0",
		"@vscode/test-electron": "^2.2.2",
		"@vscode/vsce": "^2.16.0",
		"mocha": "^10.2.0",
		"typescript": "^4.9.4"
	},
	"dependencies": {
		"glob": "^7.2.3"
	}
}
