{
	"name": "python-init-generator",
	"displayName": "Python init Generator",
	"description": "Generate Python __init__.py",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"license": "MIT",
	"activationEvents": [
		"onCommand:extension.generateInit"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.generateInit",
				"title": "Python init Generator: Generate __init__.py"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/chai": "^4.2.3",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"chai": "^4.2.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typemoq": "^2.1.0",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	}
}
